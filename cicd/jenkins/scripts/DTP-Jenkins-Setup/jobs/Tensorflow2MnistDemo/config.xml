                  <patternSeparator>[, ]+</patternSeparator>
                  <execCommand># Running the Script  On AI VM: 

cd $TARGET_FOLDER  &amp;&amp;  $ADD_PATH &amp;&amp;  ls -latr  &amp;&amp;  echo &quot;Running Tensorflow2 Mnist Example&quot; &amp;&amp; pwd &amp;&amp;
 python3 $RUN_TENSORFLOW_SCRIPT</execCommand>
